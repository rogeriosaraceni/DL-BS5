---
import Layout from '@layouts/LayoutSidebar.astro';
import PageTitle from '@components/ui/PageTitle.astro';
import ContentInfo from './_content-info.astro';
import ContentTab from './_content-tab.astro';

const breadcrumbs = [{ label: 'Home', href: '/' }, { label: 'My Plugins' }, { label: 'TabFlat' }];
---

<Layout>
    <PageTitle title="TabFlat" breadcrumbs={breadcrumbs} />

    <div class="card">
        <div class="card-body">
            <ContentInfo />
            <ContentTab />
        </div>
    </div>
</Layout>
